<?xml version="1.0" encoding="utf-8" ?>
<configuration>

	<configSections>
		<section name ="entityFramework"
			 type="System.Data.Entity.Internal.Configure.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral"
			 requirePermission="false" />
	</configSections>
	
	<connectionStrings>
		<add name="CBConnection" connectionString="Server=localhost; Database=LojaOnlineDB; Trusted_Connection=True;" providerName="System.Data.SqlClient"/>
	</connectionStrings>

	<entityFramework>
		<providers>
			<provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer"/>
		</providers>
	</entityFramework>
</configuration>

